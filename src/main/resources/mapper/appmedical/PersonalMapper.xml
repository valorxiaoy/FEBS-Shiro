<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cc.mrbird.febs.appmedical.mapper.PersonalMapper">
    <update id="updatePhoneByKey" parameterType="cc.mrbird.febs.organizationalmanagement.entity.Staff">
        update t_staff_table
        <set>
            <if test="staffName!=null">
                staff_name =#{staffName},
            </if>
            <if test="gender != null">
                gender =#{gender},
            </if>
            <if test="birthday != null">
                birthday =#{birthday},
            </if>
            <if test="age != null">
                age =#{age},
            </if>
            <if test="updateTime != null">
                update_time =#{updateTime},
            </if>
            <if test=" phone !=null">
                phone =#{phone},
            </if>
            <if test="subsidiaryOrgan">
                subsidiary_organ =#{subsidiaryOrgan}
            </if>
        </set>
            WHERE phone =#{phone}
    </update>

    <select id="selectPhoneByKey" parameterType="java.lang.Integer"
            resultType="cc.mrbird.febs.organizationalmanagement.entity.Staff">
        select  staff_name,gender,birthday,age,phone,subsidiary_organ from t_staff_table where phone =#{phone}
    </select>

</mapper>